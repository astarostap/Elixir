

<%= javascript_include_tag "searcher" %>
<input id="search_box" type="text" style="position:relative;top:100px;"></input>
<div id="resultBox"></div>
<script type="text/javascript">
  //<![CDATA[
    new Searcher("search_box", "resultBox", "/questions/query"); 
  //]]>
</script>

<div id="owl-demo" class="owl-carousel" style="position:relative;top:52px;">
 

<% @all_questions.each_with_index do |q, index| %>
      <div class="item">
        <%= link_to(image_tag(q.photo_link, :style => "width:100%; height:500px"), q.photo_link) %>
        <div class="carousel-caption">
          <h3><%= q.title %></h3>
        </div>
      </div>
      <% end %>

</div>

<script src="assets/javascripts/owl-carousel.min.js"></script>
<script src="assets/javascripts/custom.js"></script>

